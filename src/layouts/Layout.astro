<style is:global lang="scss">
  :root {
    --blue: #4b99b8;
    --light-blue: #ade0f5;
    --dark-blue: #065989;
    --dark-blue2: #013e62;
    --brown: #467a98;
    --yellow: #d9e38a;
    --green: #5ebb7e;
    --orange: #eaa674;
    --font: "Josefin Sans", sans-serif;
    --transition: all 0.3s ease-in-out;
  }
  html {
    // font-size: 16px;
    @media screen and (max-width: 768px) {
      font-size: 14px !important;
    }

    @media screen and (max-width: 1080px) and (min-width: 769px) {
      font-size: 15px;
    }
    @media screen and (max-width: 1280px) and (min-width: 1081px) {
      font-size: 15px;
    }

    @media screen and (max-width: 1440px) and (min-width: 1281px) {
      font-size: 16px;
    }

    @media screen and (min-width: 1441px) {
      // font-size: 18px;
    }
  }

  .new-wa {
    position: fixed;
    right: 30px;
    bottom: 30px;
    width: 60px;
    height: 60px !important;
    border-radius: 10px;
    height: auto;
    background-color: rgb(18, 220, 82);
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 11;

    @media screen and (max-width: 500px) {
      width: 60px;
      height: 60px !important;
      right: 0px;

      svg {
        width: 40px !important;
        height: 40px !important;
        fill: white;
      }
    }
    // background-color: red;

    svg {
      width: 40px;
      height: 40px;
      fill: white;
    }
  }

  body {
    // font-size: 16px;
    @media screen and (max-width: 768px) {
      font-size: 14px !important;
    }

    @media screen and (max-width: 1080px) and (min-width: 769px) {
      font-size: 15px;
    }
    @media screen and (max-width: 1280px) and (min-width: 1081px) {
      font-size: 15px;
    }

    @media screen and (max-width: 1440px) and (min-width: 1281px) {
      font-size: 16px;
    }

    @media screen and (min-width: 1441px) {
      // font-size: 18px;
    }
  }

  .container {
    max-width: 1280px;

    @media screen and (max-width: 1440px) and (min-width: 1280px) {
      padding: 0 2rem;
    }

    @media screen and (max-width: 1280px) and (min-width: 1080px) {
      padding: 0 2rem;
    }

    @media screen and (max-width: 1080px) and (min-width: 769px) {
      padding: 0 2rem;
    }

    @media screen and (max-width: 768px) {
      padding: 0 2rem;
    }
  }

  a {
    color: inherit;
    text-decoration: none;
  }

  ul {
    padding: 0;
    margin: 0;
  }

  * {
    box-sizing: border-box;
  }

  body {
    line-height: 1.6;
    font-weight: 500 !important;
    font-family: var(--font);
    // background-color: #f0f4f6;
    grid-auto-flow: column;
    transition: all 0.5s ease;

    font-size: 18px;

    margin: 0 !important;
    padding: 0;

    overflow-x: hidden;
  }

  html {
    scroll-behavior: smooth;
    scroll-padding-top: 100px;
  }

  .brown {
    color: var(--brown);
  }

  .sub-title {
    color: var(--d-9-d-9-d-9, #29749e);
    // text-align: center;
    font-family: var(--font);
    font-size: clamp(22px, 4vw, 25px);
    font-style: normal;
    font-weight: 700 !important;
    line-height: normal;

    h1 {
      color: var(--d-9-d-9-d-9, #29749e);
      // text-align: center;
      font-family: Helvetica;
      font-size: clamp(22px, 4vw, 25px);
      font-style: normal;
      font-weight: 700;
      line-height: normal;
    }
  }

  .class-image {
    width: 100%;
    height: 50vh;
    object-fit: cover;
  }
</style>
